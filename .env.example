# Environment variables template
# Copy this file to .env and fill in your values

# For reading data from Google Sheets
VITE_GOOGLE_SHEETS_API_KEY=your_api_key_here
VITE_SPREADSHEET_ID=your_spreadsheet_id_here
VITE_SHEET_ID=your_sheet_id_here

# For writing data to Google Sheets (required for Add Member feature)
# You need to deploy a Google Apps Script Web App - see GOOGLE_APPS_SCRIPT_SETUP.md
VITE_GOOGLE_SCRIPT_WEB_APP_URL=your_web_app_url_here
